<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hackfest">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/logohackfest"
        android:label="@string/app_name"
        android:focusableInTouchMode="true"
        android:usesCleartextTraffic="true"
        android:roundIcon="@drawable/logohackfest"
        android:supportsRtl="true"
        android:theme="@style/Theme.HackFest">
        <uses-library android:name="org.apache.http.legacy" android:required="false"/>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Location"/>
        <activity android:name=".MainMenu"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".Encrypt"/>
        <activity android:name=".Decrypt"/>
        <activity android:name=".WiFiHack"/>
        <activity android:name=".getIp"/>

    </application>

</manifest>